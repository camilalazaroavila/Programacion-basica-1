package tp2;

import tp2.dominio.Persona;

public class EjemploDeVida {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Persona sofia= new Persona("Sofia", "mujer", 38432153, 2.8, 0.50);		
