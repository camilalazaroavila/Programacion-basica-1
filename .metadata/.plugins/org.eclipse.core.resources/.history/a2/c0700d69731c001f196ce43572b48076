package tp2.dominio;

public class Persona {
	private String nombre;
	private String genero;
	private int dni;
	private double peso;
	private double altura;
	
	public Persona(String nombre, String genero, int dni, double peso, double altura) {
		this.nombre = nombre;
		this.genero = genero;
		this.dni = dni;
		this.peso = peso;
		this.altura = altura;
	}
	

}
